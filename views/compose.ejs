<%- include("partials/header"); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
      <input type="text" class="form-control" name="postTitle" autocomplete="off">
  </div>

<div class="form-group">
  <label>Post</label>
  <textarea class= "form-control" name="postArea" rows="5" cols="150" autocomplete= "off"></textarea>
</div>


<button class = "btn btn-large btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer"); -%>
